{
  "name": "@confy/sample-project",
  "version": "0.1.24",
  "description": "A sample project using Confy.",
  "private": true,
  "scripts": {
    "init": "confy-core init --env=development",
    "start": "confy-core start --env=development",
    "build": "confy-core build --env=production",
    "build-watch": "confy-core build --watch --env=production",
    "test": "confy-core test --env=test",
    "lint": "eslint \"src/**\"",
    "config": "confy-core show-config --env=development"
  },
  "license": "MIT",
  "devDependencies": {
    "@confy/core": "^1.0.0-alpha.0",
    "@confy/preset-react": "^1.0.0-alpha.0",
    "@confy/preset-sass": "^0.1.2",
    "@confy/preset-typescript": "^1.0.0-alpha.0"
  },
  "dependencies": {
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9"
  }
}
